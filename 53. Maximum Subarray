/*  
[-2,1,-3,4,-1,2,1,-5,4]
sol1:
O(n^2)

sol2:
O(n^2)
subarraySum

sol3:
*/
class Solution {
    public int maxSubArray(int[] nums) {
        int max = nums[0];
        int preSubsum = nums[0];
        for (int i = 1; i < nums.length; i++) {
            preSubsum = preSubsum < 0 ? nums[i] : nums[i] + preSubsum;
            max = Math.max(max, preSubsum);
        }
        return max;
    }
}
